class Solution {
    public int lengthOfLastWord(String s) {
        int count =0; int i = s.length()-1;
        for(int x = i; x>=0;x--){
            if(s.charAt(x) != ' '){
                count++;
            }else{
                if(count > 0)
                return count;
            }
        }
        return count;
    }
}
